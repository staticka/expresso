{% extends 'e.layout.twig' %}

{% block content %}
  <h1 style="margin-top: 20px; margin-bottom: 20px;">Pages</h1>
  <ul class="list-group">
    {% for item in pages %}
        <a href="/pages?id={{ item.filename }}" class="list-group-item list-group-item-action">{{ item.title }}</a>
    {% endfor %}
  </ul>
{% endblock %}