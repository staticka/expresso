{
    "name": "staticka/expresso",
    "description": "A flat-file and static blogging platform.",
    "keywords": ["blogging", "expresso", "flat-file", "static site"],
    "homepage": "https://github.com/staticka/expresso",
    "license": "MIT",
    "authors": [
        {
            "name": "Rougin Gutib",
            "email": "rougingutib@gmail.com",
            "homepage": "https://rougin.github.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "staticka/staticka": "~0.1",
        "twig/twig": "~1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.2|~5.7",
        "scrutinizer/ocular": "~1.1.0"
    },
    "bin": [
        "bin/expresso",
        "bin/expresso.php"
    ],
    "autoload": {
        "psr-4": {
            "Staticka\\Expresso\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Staticka\\Expresso\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}