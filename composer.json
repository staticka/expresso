{
  "name": "staticka/expresso",
  "description": "Static blog platform based on Staticka.",
  "keywords": [ "static-blog", "php-blog", "static-site" ],
  "homepage": "https://roug.in/staticka/expresso/",
  "license": "MIT",
  "authors":
  [
    {
      "email": "rougingutib@gmail.com",
      "homepage": "https://roug.in/",
      "name": "Rougin Gutib",
      "role": "Software Engineer"
    }
  ],
  "require":
  {
    "php": ">=5.3.0",
    "staticka/staticka": "dev-master"
  },
  "require-dev":
  {
    "phpunit/phpunit": "~4.2|~5.7|~6.0|~7.0|~8.0|~9.0",
    "sanmai/phpunit-legacy-adapter": "~6.1|~8.0",
    "vlucas/valitron": "~1.4"
  },
  "autoload":
  {
    "psr-4":
    {
      "Staticka\\Expresso\\": "src"
    }
  },
  "autoload-dev":
  {
    "psr-4":
    {
      "Staticka\\Expresso\\": "tests"
    }
  },
  "extra":
  {
    "branch-alias":
    {
      "dev-master": "1.0-dev"
    }
  }
}